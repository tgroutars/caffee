{
  "name": "caffee",
  "version": "0.0.1",
  "main": "server/server.js",
  "scripts": {
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "dev-server": "cross-env NODE_ENV=development nodemon -x \"node -r dotenv/config bin/www\"",
    "dev": "npm run dev-server",
    "format": "npm run prettier && npm run lint:fix",
    "lint:fix": "eslint --ext .js,.jsx --fix server",
    "lint": "eslint --ext .js,.jsx server",
    "migration:create": "sequelize migration:create",
    "prettier": "prettier --write \"server/**/*.js\"",
    "start": "node bin/www"
  },
  "repository": "https://github.com/tgroutars/caffee-old/settings",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^4.3.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "ioredis": "^3.2.2",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-sendfile": "^2.0.0",
    "koa-sslify": "^2.1.2",
    "koa-static": "^4.0.3",
    "kue": "^0.11.6",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.1.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4"
  }
}

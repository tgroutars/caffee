{
  "name": "caffee",
  "version": "0.0.1",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "dev-server": "cross-env NODE_ENV=development nodemon -x \"node -r dotenv/config bin/www\"",
    "dev-client": "parcel ./client/index.html",
    "format": "npm run prettier && npm run lint:fix",
    "lint:fix": "eslint --ext .js,.jsx --fix server client",
    "lint:strict": "eslint --max-warnings=0 --ext .js,.jsx server client",
    "lint": "eslint --ext .js,.jsx server client",
    "migration:create": "sequelize migration:create",
    "prettier": "prettier --write \"server/**/*.js\"",
    "start": "node bin/www",
    "build": "parcel build ./client/index.html",
    "heroku-postbuild": "npm run build"
  },
  "repository": "https://github.com/tgroutars/caffee-old/settings",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "@slack/client": "^4.3.1",
    "antd": "^3.8.1",
    "aws-sdk": "^2.282.1",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bee-queue": "^1.2.2",
    "bluebird": "^3.5.1",
    "connected-react-router": "^4.3.0",
    "ejs": "^2.6.1",
    "form-data": "^2.3.2",
    "fuse.js": "^3.2.1",
    "history": "^4.7.2",
    "hubspot": "^1.3.2",
    "ioredis": "^3.2.2",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-sendfile": "^2.0.0",
    "koa-sslify": "^2.1.2",
    "koa-static": "^5.0.0",
    "less": "^2.7.2",
    "lodash": "^4.17.10",
    "mime-types": "^2.1.19",
    "normalizr": "^3.2.4",
    "oauth": "^0.9.15",
    "parcel-bundler": "^1.9.7",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^16.4.2",
    "react-redux": "^4.4.8 || ^5.0.4",
    "react-router-dom": "^4.3.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "styled-components": "^3.4.2",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "concurrently": "^3.6.1",
    "cross-env": "^5.1.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4"
  }
}

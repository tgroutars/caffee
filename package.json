{
  "name": "caffee",
  "version": "0.0.1",
  "main": "server/server.js",
  "scripts": {
    "dev": "npm run dev-server",
    "lint": "eslint --ext .js,.jsx server",
    "lint:fix": "eslint --ext .js,.jsx --fix server",
    "prettier": "prettier --write \"server/**/*.js\"",
    "format": "npm run prettier && npm run lint:fix",
    "start": "node bin/www",
    "dev-server": "cross-env NODE_ENV=development nodemon -x \"node -r dotenv/config bin/www\""
  },
  "repository": "https://github.com/tgroutars/caffee-old/settings",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-sendfile": "^2.0.0",
    "koa-sslify": "^2.1.2",
    "koa-static": "^4.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.1.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4"
  }
}
